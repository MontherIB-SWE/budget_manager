{
  "version": 2,
  "builds": [
    {
      "src": "package.json",
      "use": "@vercel/static-build",
      "config": { "distDir": "dist/budget-management/browser" }
    }
  ],
  "routes": [
    { "src": "/assets/(.*)", "dest": "/assets/$1" },
    { "src": "/favicon.ico", "dest": "/favicon.ico" },
    { "src": "/(.*)\\.(js|css|png|jpg|jpeg|gif|svg|webp|ico|map)", "dest": "/$1.$2" },
    { "src": "/(.*)\\.(mp4|webm|ogg|ogv|mp3|wav)", "dest": "/$1.$2" },
    { "src": "/(.*)", "dest": "/index.html" }
  ]
}